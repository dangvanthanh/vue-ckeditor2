{
  "name": "vue-ckeditor2",
  "version": "1.20.0",
  "description": "Ckeditor 4 using with Vue.js",
  "author": "Dang Van Thanh <dangvanthanh@dangthanh.org>",
  "main": "src/components/Ckeditor.vue",
  "peerDependencies": {
    "vue": ">= 2",
    "ckeditor": ">= 4"
  },
  "repository": {
    "type": "git",
    "url": "vue-ckeditor"
  },
  "keywords": [
    "vue",
    "ckeditor",
    "vue-ckeditor"
  ],
  "scripts": {
    "build": "run-s build:**",
    "build:js": "cross-env NODE_ENV=production rollup -c",
    "build:autoprefixer": "postcss --use autoprefixer -o dist/assets/css/app.css dist/assets/css/app.css",
    "build:cssnano": "cssnano dist/assets/css/app.css dist/assets/css/app.css",
    "dev": "cross-env NODE_ENV=development rollup -cw"
  },
  "dependencies": {
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "cross-env": "^5.0.5",
    "cssnano-cli": "^1.0.4",
    "npm-run-all": "^4.0.1",
    "postcss-cli": "^4.0.0",
    "rollup": "^0.53.2",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^3.0.0",
    "vue-template-compiler": "^2.5.13"
  },
  "license": "MIT"
}
