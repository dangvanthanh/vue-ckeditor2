{
  "name": "vue-ckeditor2",
  "version": "3.0.0",
  "description": "Ckeditor 4 using with Vue.js",
  "main": "dist/vue-ckeditor2.umd.js",
  "module": "dist/vue-ckeditor2.esm.js",
  "unpkg": "dist/vue-ckeditor2.min.js",
  "scripts": {
    "dev": "parcel index.html",
    "build": "npm run build:browsers && npm run build:es && npm run build:umd",
    "build:browsers": "rollup -c build/rollup.config.browsers.js",
    "build:es": "rollup -c build/rollup.config.es.js",
    "build:umd": "rollup -c build/rollup.config.umd.js",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dangvanthanh/vue-ckeditor2.git"
  },
  "keywords": [
    "vue",
    "ckeditor",
    "vue-ckeditor"
  ],
  "author": "Dang Van Thanh <dangvanthanh@dangthanh.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dangvanthanh/vue-ckeditor2/issues"
  },
  "homepage": "https://github.com/dangvanthanh/vue-ckeditor2#readme",
  "lint-staged": {
    "*.{js,json,vue},!dist/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.8",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/vue": "^5.8.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.0.6",
    "clean-css": "^5.1.3",
    "esbuild": "^0.12.15",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.2",
    "rollup": "^2.54.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-vue": "^5.1.9",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.14"
  },
  "dependencies": {
    "vue": "^2.6.14"
  }
}
